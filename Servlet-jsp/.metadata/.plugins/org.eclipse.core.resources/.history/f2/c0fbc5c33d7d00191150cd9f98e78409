package com.djs.entity;

public class Pet {
	int id;
	String kind;
	String color;
	double price;
	
	public Pet (int id,String kind,String color,double price) {
		this.id = id;
		this.kind = kind;
		this.color = color;
		this.price = price;
	}
	@Override
	public String toString() {
		return "Pet [id=" + id + ", kind=" + kind + ", color=" + color + ", price=" + price + "]";
	}
	
	

}
